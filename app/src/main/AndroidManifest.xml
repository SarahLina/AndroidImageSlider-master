<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mac.miniprojet" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="JULINA"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListProductsActivity" />
        <activity android:name=".ListCategoriesActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".DetailsProductActivity" />
        <activity android:name=".PurchaseActivity" />
        <activity android:name=".SettingsActivity" />
    </application>

</manifest>